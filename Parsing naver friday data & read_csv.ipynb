    "url = 'http://comic.naver.com/webtoon/weekdayList.nhn?week=fri'\n",
    "r = urllib2.urlopen(url)\n",
    "soup = BT(r,'lxml')\n",
    "data = soup.findAll('dt')\n",
    "href = list()\n",
    "for i in range(len(data)):\n",
    "    href.append(data[i].find('a')['href'])\n",
    "href=list(set(href))"
